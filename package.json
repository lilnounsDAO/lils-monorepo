{
  "name": "lils-monorepo",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*",
    "tooling/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "clean": "turbo run clean",
    "test": "turbo run test",
    "type-check": "turbo run type-check",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format-check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "changeset": "changeset",
    "version-packages": "changeset version",
    "release": "turbo run build && changeset publish"
  },
  "dependencies": {
    "@payloadcms/richtext-lexical": "3.13.0",
    "@snapshot-labs/snapshot.js": "0.14.14",
    "ethers": "6.15.0",
    "sharp": "0.34.5",
    "uint8array-extras": "^1.3.0"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.1",
    "@turbo/gen": "^1.11.2",
    "eslint": "^8.57.0",
    "prettier": "^3.1.1",
    "turbo": "^1.11.2",
    "typescript": "^5.3.3",
    "@types/react": "18.3.12",
    "@types/react-dom": "18.3.1",
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "packageManager": "bun@1.1.34",
  "engines": {
    "node": ">=18"
  },
  "description": "A modern Turborepo monorepo for the Lil Nouns DAO ecosystem, featuring the main web application, and CMS.",
  "main": "fetch-nouns-data.js",
  "directories": {
    "doc": "docs"
  },
  "keywords": [],
  "author": "lil nouns",
  "homepage": "https://lilnouns.wtf"
  "license": "GPL-3.0",
}
